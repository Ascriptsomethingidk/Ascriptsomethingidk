	game:GetService("Players").LocalPlayer.PlayerGui.GUI.Animations.ScrollingFrame.Explode.Visible = true
	game:GetService("Players").LocalPlayer.PlayerGui.GUI.Animations.ScrollingFrame.Higher.Visible = true
	game:GetService("Players").LocalPlayer.PlayerGui.GUI.Animations.ScrollingFrame.Lower.Visible = true
	game:GetService("Players").LocalPlayer.PlayerGui.GUI.Animations.ScrollingFrame.Shoot.Visible = true
	game:GetService("Players").LocalPlayer.PlayerGui.GUI.Animations.ScrollingFrame.SpeedUp.Visible = true
         end
      end
})

AllianceTab:AddButton({
    Name = "Anti Parasite Tank No Cooldown",
    Callback = function()
              while true do
                  wait(0.1)
	game:GetService("Players").LocalPlayer.PlayerGui.GUI.Animations.ScrollingFrame.AimHigh.Visible = true
	game:GetService("Players").LocalPlayer.PlayerGui.GUI.Animations.ScrollingFrame.Explode.Visible = true
	game:GetService("Players").LocalPlayer.PlayerGui.GUI.Animations.ScrollingFrame.Headphones.Visible = true
	game:GetService("Players").LocalPlayer.PlayerGui.GUI.Animations.ScrollingFrame.Ram.Visible = true
	game:GetService("Players").LocalPlayer.PlayerGui.GUI.Animations.ScrollingFrame.Shoot.Visible = true
	game:GetService("Players").LocalPlayer.PlayerGui.GUI.Animations.ScrollingFrame.SpeedUp.Visible = true
	game:GetService("Players").LocalPlayer.PlayerGui.GUI.Animations.ScrollingFrame["Thumbs Down"].Visible = true
	game:GetService("Players").LocalPlayer.PlayerGui.GUI.Animations.ScrollingFrame["Thumbs Up"].Visible = true
         end
      end
})




OrionLib:Init()
