local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Script For Ustr 2", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local CreditTab = Window:MakeTab({
	Name = "Credits",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false

})
CreditTab:AddButton({
	Name = "By Silly Cat!",
	Callback = function()
      		print("Nothing Why you pressed it by silly cat btw")
  	end    

})
local BonusTab = Window:MakeTab({
	Name = "Bonus!",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

BonusTab:AddButton({
	Name = "Teleport To Secret Morphs",
	Callback = function()
      		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1830, 175, -2658)
  	end    
})

BonusTab:AddButton({
	Name = "Get Gojo Moves (only works one)",
	Callback = function()
      		local Nuke = workspace["Gojo Cameraman"].Pad.GUI.Animations.ScrollingFrame.Nuke
            local clonedNuke = workspace["Gojo Cameraman"].Pad.GUI.Animations.ScrollingFrame.Nuke:Clone()
            local Blue = workspace["Gojo Cameraman"].Pad.GUI.Animations.ScrollingFrame.Blue
            local clonedBlue = workspace["Gojo Cameraman"].Pad.GUI.Animations.ScrollingFrame.Blue:Clone()

            clonedBlue.Parent = game:GetService("Players").LocalPlayer.PlayerGui.GUI.Animations.ScrollingFrame
            clonedNuke.Parent = game:GetService("Players").LocalPlayer.PlayerGui.GUI.Animations.ScrollingFrame
  	end    
})

BonusTab:AddButton({
	Name = "Get Gojo Domain",
	Callback = function()
	       workspace["Gojo Cameraman"].Pad.GUI.Animations.ScrollingFrame.Domain.Parent = game:GetService("Players").LocalPlayer.PlayerGui.GUI.Animations.ScrollingFrame
  	end    
})

BonusTab:AddButton({
	Name = "Get Sukuna Domain",
	Callback = function()
           instance = workspace["Sukuna or yuji idk"].Pad.GUI.Animations.ScrollingFrame.Domain
           instance.Name = "Domain2"
           wait(0.2)
           workspace["Sukuna or yuji idk"].Pad.GUI.Animations.ScrollingFrame.Domain2.Parent = game:GetService("Players").LocalPlayer.PlayerGui.GUI.Animations.ScrollingFrame
  	end    
})

BonusTab:AddButton({
	Name = "Get New Sukuna Domain",
	Callback = function()
            workspace["Sukuna or yuji idk"].Pad.GUI.Animations.ScrollingFrame.Domain2.Parent = game:GetService("Players").LocalPlayer.PlayerGui.GUI.Animations.ScrollingFrame
  	end    
})

BonusTab:AddButton({
	Name = "Put Sukuna Domain Back",
	Callback = function()
            game:GetService("Players").LocalPlayer.PlayerGui.GUI.Animations.ScrollingFrame.Domain2.Parent = workspace["Sukuna or yuji idk"].Pad.GUI.Animations.ScrollingFrame
  	end    
})

BonusTab:AddButton({
	Name = "Put Gojo Domain Back",
	Callback = function()
            game:GetService("Players").LocalPlayer.PlayerGui.GUI.Animations.ScrollingFrame.Domain.Parent = workspace["Gojo Cameraman"].Pad.GUI.Animations.ScrollingFrame
  	end    
})

BonusTab:AddButton({
	Name = "Putting Domains back so you can get it again",
	Callback = function()
            print("PLEASE READ")
  	end    
})

